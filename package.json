{
  "name": "armorybot",
  "version": "0.10.0",
  "type": "module",
  "description": "Discord Bot for AnneMunition's Armory",
  "private": true,
  "author": "DBKynd <db@dbkynd.com>",
  "license": "MIT",
  "engines": {
    "node": ">=22.14.0"
  },
  "engineStrict": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch -r dotenv/config ./src/index.ts",
    "build": "rimraf ./dist && tsc -p ./tsconfig.build.json",
    "start": "cross-env NODE_ENV=production node -r dotenv/config ./dist",
    "prettier": "prettier --write \"**/*.{js,ts,md,*rc,json}\" --ignore-path .gitignore",
    "lint": "eslint --ext .js,.ts --ignore-pattern .gitignore .",
    "test": "jest",
    "docker:build": "docker build -t dbkynd/armory-bot:latest ."
  },
  "dependencies": {
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs-node": "^4.22.0",
    "axios": "^1.8.4",
    "bufferutil": "^4.0.9",
    "cheerio": "^1.0.0",
    "cron": "^3.5.0",
    "discord.js": "^14.18.0",
    "emoji-regex": "^10.4.0",
    "erlpack": "^0.1.4",
    "getos": "^3.2.1",
    "html-entities": "^2.5.3",
    "humanize-duration": "^3.32.1",
    "lodash": "^4.17.21",
    "luxon": "^3.6.0",
    "mathjs": "^11.12.0",
    "mongoose": "^7.8.6",
    "node-vibrant": "3.2.1-alpha.1",
    "os": "^0.1.2",
    "pidusage": "^3.0.2",
    "rgb2hex": "^0.2.5",
    "temp-units-conv": "^1.0.2",
    "utf-8-validate": "^6.0.5",
    "winston": "^3.17.0",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/js": "^9.23.0",
    "@types/getos": "^3.0.4",
    "@types/humanize-duration": "^3.27.4",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.17.28",
    "@types/pidusage": "^2.0.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.2.5",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "mockdate": "^3.0.5",
    "nock": "^13.5.6",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.3.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  }
}
